<svg width="180" height="180" viewBox="0 0 180 180" fill="none" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="gradient1" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#6366f1;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#8b5cf6;stop-opacity:1" />
    </linearGradient>
    <linearGradient id="gradient2" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#ec4899;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#f97316;stop-opacity:1" />
    </linearGradient>
  </defs>

  <!-- Background with rounded corners for Apple icon -->
  <rect width="180" height="180" rx="40" fill="url(#gradient1)"/>

  <!-- Main character figure (scaled up) -->
  <g transform="translate(90, 90) scale(5.625)">
    <!-- Head -->
    <circle cx="-3" cy="-6" r="2.5" fill="#ffffff"/>

    <!-- Body -->
    <rect x="-4" y="-3" width="2" height="6" rx="1" fill="#ffffff"/>

    <!-- Arms -->
    <rect x="-6" y="-2" width="2" height="1" rx="0.5" fill="#ffffff"/>
    <rect x="-2" y="-2" width="2" height="1" rx="0.5" fill="#ffffff"/>

    <!-- Legs -->
    <rect x="-4" y="3" width="1" height="3" rx="0.5" fill="#ffffff"/>
    <rect x="-3" y="3" width="1" height="3" rx="0.5" fill="#ffffff"/>
  </g>

  <!-- Motion lines/animation effect (scaled up) -->
  <g opacity="0.8" transform="scale(5.625)">
    <path d="M8 8 L10 6" stroke="url(#gradient2)" stroke-width="1.5" stroke-linecap="round"/>
    <path d="M6 12 L8 10" stroke="url(#gradient2)" stroke-width="1.5" stroke-linecap="round"/>
    <path d="M24 8 L22 6" stroke="url(#gradient2)" stroke-width="1.5" stroke-linecap="round"/>
    <path d="M26 12 L24 10" stroke="url(#gradient2)" stroke-width="1.5" stroke-linecap="round"/>
  </g>

  <!-- Play button overlay (scaled up) -->
  <g transform="translate(123.75, 123.75)" opacity="0.9">
    <circle cx="0" cy="0" r="22.5" fill="url(#gradient2)"/>
    <polygon points="-8.4375,-8.4375 -8.4375,8.4375 8.4375,0" fill="#ffffff"/>
  </g>

  <!-- W character overlay (scaled up) -->
  <text x="22.5" y="45" font-family="Arial, sans-serif" font-size="33.75" font-weight="bold" fill="#ffffff">W</text>
</svg>